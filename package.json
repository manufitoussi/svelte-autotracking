{
  "name": "svelte-autotracking",
  "version": "0.1.2",
  "description": "An autotracking solution to simplify using stores in Svelte with class objects",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manufitoussi/svelte-autotracking.git"
  },
  "homepage": "https://github.com/manufitoussi/svelte-autotracking",
  "type": "module",
  "keywords": [
    "svelte",
    "store",
    "autotracking",
    "class",
    "tracked"
  ],
  "author": "Emmanuel FITOUSSI",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest run",
    "coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest"
  },
  "files": [
    "dist"
  ],
  "main": "dist/svelte-autotracking.es.js",
  "typings": "dist/index.d.ts",
  "module": "dist/svelte-autotracking.es.js",
  "exports": {
    ".": {
      "import": "./dist/svelte-autotracking.es.js",
      "require": "./dist/svelte-autotracking.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/svelte": "^5.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@tsconfig/svelte": "^5.0.2",
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "jsdom": "^24.0.0",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.7",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.0",
    "vitest": "^1.6.0"
  }
}
